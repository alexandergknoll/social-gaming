<%= form_for user do |f| %>
	<table>
		<tr>
			<td><%= f.label :name %></td>
			<td><%= f.text_field :name %></td>
		</tr>
		<tr>
			<td><%= f.label :username %></td>
			<td><%= f.text_field :username %></td>
		</tr>
		<tr>
			<td><%= f.label :email %></td>
			<td><%= f.email_field :email %></td>
		</tr>
		<tr>
			<td><%= f.label :password %></td>
			<td><%= f.password_field :password %></td>
		</tr>
		<tr>
			<td></td>
			<td><%= f.submit %></td>
		</tr>
	</table>
<% end %>